    <PackageReference Include="Microsoft.NETCore.App" Version="1.0.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Shed.Controllers\Shed.Controllers.csproj" />
  </ItemGroup>
</Project>